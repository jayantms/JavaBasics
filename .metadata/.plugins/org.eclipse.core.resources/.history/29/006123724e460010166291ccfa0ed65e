package basics.codingpractice;

import java.util.ArrayList;
import java.util.List;
import java.util.Set;

public class StringSorting {

	public static void main(String[] args) {
		List<String> lstrList= new ArrayList<String>(); 
		
		lstrList.add("lmono");
		lstrList.add("pqrs");
		lstrList.add("abcd"); 
		lstrList.add("zctv");
		lstrList.add("rtyv");
		
		Set<String> lhashset = new SortedSet<String>(); 
		
		lstrList.forEach(str -> lhashset.add(str));
		
		lhashset.forEach(str -> System.out.println(str));
	}
}
