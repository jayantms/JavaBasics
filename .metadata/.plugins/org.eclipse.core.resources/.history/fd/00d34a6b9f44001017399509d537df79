package basics.collections.multithreadedapp;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
    	ThreadHandling t1 = new ThreadHandling(); 
    	
    	
    	WorkingUnit w1 = new WorkingUnit(); 
    	Thread t1 = new Thread(w1);
    	
    	t1.start();
    
    }
}
