package basics.collections.listinterface;

import java.util.ArrayList;
import java.util.List;

public class ArrayListExample 
{
    public static void main( String[] args )
    {
        List<Integer> larrayList = new ArrayList<Integer>(); 
        
        larrayList.add(1); 
        larrayList.add(2); 
        larrayList.add(3); 
        larrayList.add(4); 
        
        for(Integer i: larrayList) {
        	System.out.println(i); 
        }
        
        /////////////////
        
        Employee l1 = new Employee(1,"Abcd"); 
        Employee l2 = new Employee(2,"Pqrs"); 
        Employee l3 = new Employee(3,"rstv"); 
        Employee l4 = new Employee(4,"lmno"); 
        
        ArrayList<Employee> lempList = new ArrayList<Employee>(); 
        lempList.add(l1); 
        lempList.add(l2); 
        lempList.add(l3); 
        lempList.add(l4); 
        
        for(Employee lemp: lempList) {
        	System.out.println(lemp.toString()); 
        }
     
        ////////////////////// Lamda expressions 
        
        lempList.forEach(emp -> System.out.println(emp.toString()));
        
    }
}
