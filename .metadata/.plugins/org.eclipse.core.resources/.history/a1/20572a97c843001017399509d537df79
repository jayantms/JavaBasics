package basics.collections.comparable;

import java.util.ArrayList;
import java.util.List;

import basics.collections.mapinterface.Employee;

public class App 
{
    public static void main( String[] args )
    {
    	List<Employee> tlist = new ArrayList<Employee>(); 
    	
        Employee l1 = new Employee(1,"Abcd"); 
        Employee l2 = new Employee(2,"Pqrs"); 
        Employee l3 = new Employee(3,"rstv"); 
        Employee l4 = new Employee(4,"lmno"); 
        
        tlist.add(l1);  
        tlist.add(l2);  
        tlist.add(l3);  
        tlist.add(l4);  

        
    }
}
