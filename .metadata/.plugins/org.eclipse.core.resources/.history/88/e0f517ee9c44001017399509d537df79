package basics.collections.multithreadedapp;

public class WorkingUnit implements Runnable {

	public void run() {
		for(int i=0 ; i < 10 ; i++) {
			System.out.println("Test - i");
			try {
				Thread.sleep(100);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}
}
